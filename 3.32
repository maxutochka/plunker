        		<script>
function clean(){
var canvas = document.getElementById('example');
var ctx = canvas.getContext('2d');
ctx.fillStyle = "white";
ctx.fillRect(0, 0, 640, 480);
ctx.closePath();

}
		function er1(){
var canvas = document.getElementById('example');
var ctx = canvas.getContext('2d');
ctx.beginPath();
ctx.strokeStyle = 'black';
ctx.moveTo(320, 0);
ctx.lineTo(320, 480);
ctx.moveTo(0, 240);
ctx.lineTo(640, 240);

ctx.moveTo(300, 230);
ctx.lineTo(300, 0);
ctx.moveTo(300, 230);
ctx.lineTo(0, 230);
ctx.stroke();
}
		function er2(){
